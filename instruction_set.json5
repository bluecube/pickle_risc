    "Compare and swap": {
        syntax: "cas Raddress, Rexpected, Rnew",
        encoding: ["01", "10010", "Rexpected[3]", "Raddress[3]", "Rnew[3]"],
        pseudocode: [
            "if [Raddress] == Rexpected:",
            "    [Raddress] = Rnew",
            "else:",
            "    Rnew = [Raddress]"
        ],
        cycles: 3,
        "_microcode": [
            "Tmp1 = [Raddress]",
            "cmp Rold, Tmp1; Instr = [Pc + 1]; Pc += 1",
            "if eq: [Raddress] = Rnew ; end ; else Rnew = Tmp1 ; end"
        ]
    },
