        encoding: ["1110", "0", "C[3]", "xxxxx", "Ra[3]"],
        pseudocode: "Ra = C",
        cycles: 1
    },
    "Store control register": {
        syntax: "stc Ra, C",
        encoding: ["1110", "1", "C[3]", "xxxxx", "Ra[3]"],
        pseudocode: "C = Ra",
        cycles: 1
    },

